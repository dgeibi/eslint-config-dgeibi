{
  "name": "eslint-config-dgeibi",
  "version": "5.1.1",
  "description": "dgeibi's personal ESLint config",
  "main": "index.js",
  "author": "dgeibi",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "precommit": "lint-staged",
    "format": "prettier --write \"**/*.js\""
  },
  "peerDependencies": {
    "eslint": "^4.19.0 || ^5.2.0"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7"
  },
  "dependencies": {
    "babel-eslint": "^9.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/eslint-config-dgeibi.git"
  },
  "bugs": {
    "url": "https://github.com/dgeibi/eslint-config-dgeibi/issues"
  },
  "homepage": "https://github.com/dgeibi/eslint-config-dgeibi#readme",
  "files": [
    "rules/",
    "*.js"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
